<launch>

<arg name="use_camera_info" default="false" doc="Indicates that the camera_info topic should be subscribed to to get the default input_frame_id. Otherwise the frame from the image message will be used." />
<arg name="debug_view" default="true" doc="Specify whether the node displays a window to show edge image" />

  <!-- Experiment Playback -->
<param name ="/use_sim_time" value="true"/>  
<node pkg="rosbag" type="play" name="player" output="screen" 
args="--clock -r 0.02 -s 7 -u 7 /mnt/hgfs/Experiment_Data_MIT/bucket_video_test_2017-12-06-18-49-56.bag"/>

<!-- Crop Image Node -->
<node pkg="opt_flow" type="image_transform_readCal.py" name="image_transform" output="screen">
    <remap from="UBC_Image"  to="/camera/color/image_raw" />
    <remap from="cal_Data"  to="/joint_states_dynamixel" />
    <param name="Filename" value="CalibrationFile1.txt"/>
        <rosparam>
     </rosparam>
</node>  

</launch>